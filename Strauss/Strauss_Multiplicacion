<html>
	<script>
		var arreglo=[];
		var arreglo2=[];
		var cantidad = 0;
		var condicion = 0;
		
		function Mostrar()
		{
			orden =document.getElementById("Orden").value;
			var tabla= document.getElementById("OperadorA");	
				for(i=0;i<orden;i++)
				{
					var fila=tabla.insertRow(-1);
						for(j=0;j<orden;j++)
						{
							var posicion= ""+i+j;
							var celda_individual=fila.insertCell(-1);
							var  CELDA="<input type=\"text\"";
							CELDA=CELDA+"id=\""+posicion+"\"";
							CELDA=CELDA+"size=\"5\">";
							celda_individual.innerHTML=CELDA;
						}
				}
		}
		
		function Mostrar2()
		{
			orden =document.getElementById("Orden").value;
			var tabla2= document.getElementById("OperadorB");	
				for(a=0;a<orden;a++)
				{
					var fila2=tabla2.insertRow(-1);
						for(b=0;b<orden;b++)
						{
							var posicion2= " "+a+b;
							var celda_individual2=fila2.insertCell(-1);
							var  CELDA2="<input type=\"text\"";
							CELDA2=CELDA2+"id=\""+posicion2+"\"";
							CELDA2=CELDA2+"size=\"5\">";
							celda_individual2.innerHTML=CELDA2;
						}
				}
		}
		
		function Guardar_Matriz()
		{  
			matriz= new Array(orden);
				for(i=0;i<orden;i++)
					{
						matriz[i]= new Array(orden);
							for(j=0;j<orden;j++)
							{
								var temporal=document.getElementById(""+i+j).value;
								matriz[i][j]=temporal;
								arreglo.push(temporal);
							}
					}
			return matriz;
		}
		
		function Guardar_Matriz2()
		{  
			matriz2= new Array(orden);
				for(a=0;a<orden;a++)
					{
						matriz2[a]= new Array(orden);
							for(b=0;b<orden;b++)
							{
								var temporal2=document.getElementById(" "+a+b).value;
								matriz2[a][b]=temporal2;
								arreglo2.push(temporal2);
							}
					}
			return matriz2;
		}
		
		function Constructor()
		{
			Mostrar();
			Mostrar2();
		}
		
		function Constructor2()
		{
			Strassen(Guardar_Matriz(), Guardar_Matriz2());
			console.log("A: "+arreglo);
			console.log("B: "+arreglo2);
		}
		
		function Llenar(rellenada)
		{
			cantidad=rellenada.length;
			console.log("RRR: "+rellenada);
			console.log("RRR: "+cantidad);
			matriz_Cero = new Array(cantidad);
			for (var i=0;i<cantidad;i++)
			{
				matriz_Cero[i]= new Array(cantidad);
				for (var j=0;j<cantidad;j++)
				{
					matriz_Cero[i][j]=0;
				}
			}
			return matriz_Cero;
		}
		
		function Agregar(matri, matri2)
		{
			var resultado=[];
			cantidad = matri.length;
				for(i=0;i<cantidad;i++)
					{
							for(j=0;j<cantidad;j++)
							{
									resultado[i][j]=matri[i][j]+matri2[i][j];
							}
					}
			return resultado;
		}
		
		function Multiplicar(matri, matri2)
		{
			cantidad = matri.length;
			resultado=new Array(cantidad);
			console.log("Matri: "+matri.length);
			for(i=0;i<cantidad;i++)
				{
					resultado[i]=new Array(cantidad);
					for (k=0;k<cantidad;k++)
					{
						for(j=0;j<cantidad;j++)
						{	
							if (condicion==0)
							{
								resultado=Llenar(resultado);
								condicion=1;
								console.log("RR: "+resultado[i][j])
							}
							if (condicion==1)
							{
								console.log("R: "+resultado[i][j]+" M1: "+matri[i][k]+" M2: "+matri2[k][j])
								resultado[i][j]=(matri[i][k]*matri2[k][j])+resultado[i][j];
							}	
						}
					}
				}
			return resultado;
		}
		
		function Restar(matri, matri2)
		{
			var resultado=[];
			cantidad = matri.length;
				for(i=0;i<cantidad;i++)
					{
							for(j=0;j<cantidad;j++)
							{
									resultado[i][j]=matri[i][j]-matri2[i][j];
							}
					}
			return resultado;
		}
		
		function StrassenR(matrix, matrix2)
		{
			cantidad=matrix.length;
			
			if (cantidad<=2)
			{
				return Multiplicar(matrix, matrix2);
			}
			
			else
			{
				var nueva_cantidad = cantidad/2;
				a11 = [nueva_cantidad][nueva_cantidad];
				a12 = [nueva_cantidad][nueva_cantidad];
				a21 = [nueva_cantidad][nueva_cantidad];
				a22 = [nueva_cantidad][nueva_cantidad];
				
				b11 = [nueva_cantidad][nueva_cantidad];
				b12 = [nueva_cantidad][nueva_cantidad];
				b21 = [nueva_cantidad][nueva_cantidad];
				b22 = [nueva_cantidad][nueva_cantidad];
				
				resultado_a = [nueva_cantidad][nueva_cantidad];
				resultado_b = [nueva_cantidad][nueva_cantidad];
				
					for (var i=0; i<nueva_cantidad; i++)
					{
						for (var j=0; j<nueva_cantidad; j++)
						{
							a11[i][j]=matrix[i][j];
							a12[i][j]=matrix[i][j+nueva_cantidad];
							a21[i][j]=matrix[i+nueva_cantidad][j];
							a21[i][j]=matrix[i+nueva_cantidad][j+nueva_cantidad];
							
							b11[i][j]=matrix2[i][j];
							b12[i][j]=matrix2[i][j+nueva_cantidad];
							b21[i][j]=matrix2[i+nueva_cantidad][j];
							b21[i][j]=matrix2[i+nueva_cantidad][j+nueva_cantidad];
						}
					}
				
				resultado_a=Agregar(a11,a22);
				resultado_b=Agregar(b11,b22);
				p1 = StrassenR(resultado_a, resultado_b);
				
				resultado_a=Agregar(a21, a22);
				p2 = StrassenR(resultado_a, b11);
				
				resultado_b=Restar(b12, b22);
				p3 = StrassenR(a11, resultado_b);
				
				resultado_b=Restar(b21,b11);
				p4=StrassenR(a22, resultado_b);
				
				resultado_a=Agregar(a11, a12);
				p5=StrassenR(resultado_a, b22);
				
				resultado_a= Restar(a21, a11);
				resultado_b=Agregar(b11,b12);
				p6=StrassenR(resultado_a,resultado_b);
				
				resultado_a= Restar(a12, a22);
				resultado_b=Agregar(b21,b22);
				p7=StrassenR(resultado_a, resultado_b);
				
				c12 = Agregar(p3, p5);
				c21 = Agregar(p2, p4);
				
				resultado_a=Agregar(p1, p4);
				resultado_b=Agregar(resultado_a, p7);
				c11 = Restar(resultado_b, p5);
				
				resultado_a = Agregar(p1, p3);
				resultado_b = Agregar(resultado_a, p6);
				c22 = Restar(resultado_b, p2)
				
				for (var i=0;i<cantidad;i++)
				{
					for (var j=0;j<cantidad;j++)
					{
						C[i][j] = c11[i][j]
						C[i][j + nueva_cantidad] = c12[i][j]
						C[i + nueva_cantidad][j] = c21[i][j]
						C[i + nueva_cantidad][j + nueva_cantidad] = c22[i][j]
					}
				}
				
				return C;
			}
		}
		
		function Strassen(matris, matris2)
		{
			orden =document.getElementById("Orden").value;
			matriz= new Array(orden);
			AEnviado=new Array(orden);
			BEnviado=new Array(orden);
			
			console.log("ORDEN: "+orden);
			console.log("A: "+AEnviado);
			console.log("Matris2: "+matris2)
			for (var i=0;i<orden;i++)
			{
				AEnviado[i]= new Array(orden);
				BEnviado[i]= new Array(orden);
				for (var j=0;j<orden;j++)
				{
					AEnviado[i][j]=matris[i][j];
					BEnviado[i][j]=matris2[i][j];
				}
			}
			
			console.log("PREVIA: "+AEnviado.length);
			CPreparado = StrassenR(AEnviado,BEnviado);
			
			for (var i=0;i<orden;i++)
			{
				for (var j=0;j<orden;j++)
				{
					C=CPreparado[i][j];
				}
			}
			Dibujar(C)
		}
		
		function Dibujar(resultado)
		{
			console.log(resultado);
		}
		
	</script>
	<input type="text" placeholder="Digitar cantidad de datos" id="Orden">
	<button onclick="Constructor();">Ingresar</button>
	<button onclick="Constructor2();">Calcular</button>
